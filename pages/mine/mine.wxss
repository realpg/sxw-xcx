}
.redactInfo{
  background: #00C862;
  padding: 6rpx 12rpx;
  color: #fff;
  border-radius: 10rpx;
  font-size: 22rpx;
  margin-right: 6rpx;
}
.business_card {
  background: #fff;
  width: 80%;
  margin: auto;
  padding: 3% 3% 1% 3%;
  font-size: 26rpx;
  color: #666;
  line-height: 60rpx;
  align-items: center;
  box-shadow: 0 6rpx 6rpx #888;
}

.name {
  color: black;
  font-size: 32rpx;
}

.post {
  margin-left: 20rpx;
  color: #666;
  font-size: 26rpx;
}

.The_main {
  padding: 15rpx 0 10rpx 0;
  border-top: 1rpx solid #f5f5f5;
  line-height: 46rpx;
}

.headImg {
  width: 130rpx;
  height: 130rpx;
  float: right;
  border-radius: 50%;
}

.iconImg {
  margin-right: 20rpx;
  width: 28rpx;
  height: 34rpx;
}

.options {
  display: flex;
  justify-content: space-around;
}

.optionsImg {
  width: 32rpx;
  margin-right: 10rpx;
}

.sign_in {
  background: #fff;
  width: 95;
  margin: auto;
  margin-top: 30rpx;
  line-height: 180rpx;
  padding-bottom: 10rpx;
}

.sign_in_css {
  display: flex;
  justify-content: space-around;
  font-size: 28rpx;
}

.singIn {
  background: #ff9800;
  color: white;
  padding: 30rpx 24rpx;
  border-radius: 50%;
  font-size: 26rpx;
}

.gold {
  color: #ff9800;
  font-size: 28rpx;
}

.gain {
  color: #ff9800;
}

.sign_in_date {
  display: flex;
  line-height: 50rpx;
  font-size: 26rpx;
  justify-content: space-around;
  text-align: center;
}

.sign_in_Img {
  width: 80rpx;
  height: 80rpx;
}

.each_column {
  margin-top: 20rpx;
  background: #fff;
  font-size: 28rpx;
}

.mine_item {
  display: flex;
