page {
  background: #f5f5f5;
}

.slide-swiper {
  width: 100%;
  height: 350rpx;
  z-index: 99999;
}

.slide_image {
  width: 100%;
  height: 100%;
}

.searchBox {
  width: 90%;
  margin: auto;
  height: 60rpx;
  display: flex;
  align-items: center;
  background: #f2f2f2;
  font-size: 28rpx;
  border: 1rpx solid #f2f2f2;
  border-radius: 30rpx;
  position: absolute;
  top: 30rpx;
  left: 4.5%;
  opacity: 0.8;
  /* z-index: 1000;  */
}

.searchIcon {
  width: 28rpx;
  height: 28rpx;
  margin-left: 15rpx;
}

.searchInput {
  width: 90%;
  margin-left: 10rpx;
  z-index: -1;
}

.classifyList {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  background: #fff;
  padding-bottom: 30rpx;
}

.classify {
  width: 20%;
  font-size: 30rpx;
  text-align: center;
  margin-top: 30rpx;
}

.ci {
  margin: auto;
}

.cimg {
  width: 80rpx;
  height: 80rpx;
}

.ciName {
  margin-top: 10rpx;
  font-size: 24rpx;
  color: #666;
}

.RS_list {
  font-size: 26rpx;
}
.RS_list_css{
 height: 375rpx; 
}
.recommend_store {
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.recommend_store_css {
  width: 48%;
  margin: auto;
  background: #fff;
}

.store_name {
  line-height: 80rpx;
  background: #d0dfff;
  text-align: center;
  font-size: 28rpx;
  font-weight: 700;
   display: -webkit-box; 
  align-items: center;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.store_lable {
  width: 90%;
  margin: auto;
  display: flex;
  justify-content: space-around;
  height: 80rpx;
  align-items: center;
}

.store_type {
  background: #9accf7;
  padding: 16rpx 10rpx;
  line-height: 30rpx;
  font-size: 22rpx;
  color: white;
  border-radius: 50%;
  text-align: center;
}

.lable_one {
  background: #dfffef;
  color: #40b792;
  padding: 5rpx 10rpx;
  line-height: 30rpx;
  font-size: 22rpx;
  border-radius: 5rpx;
}
.lable_two {
  font-size: 22rpx;
  background: #ffe5c0;
  color: #ffa929;
  padding: 5rpx 10rpx;
  line-height: 30rpx;
  border-radius: 5rpx;
}
.store_info{
  width: 95%;
  margin: auto;
  font-size: 26rpx;
  color: #666;
  line-height: 36rpx;
  display: -webkit-box; 
  align-items: center;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}

.Enter_store{
  height: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.Enter_store_css{
  font-size: 22rpx;
  background:#01C46C;
  color: white;
  line-height: 32rpx;
  padding: 6rpx 20rpx;
  border-radius: 6rpx;
  align-items: center;
}


.information_classification{
  background: #fff;
  display: flex;
  justify-content: space-around;
  line-height: 80rpx;
  font-size: 28rpx;
  color:  #9B9B9B; 
}
.all{
  color:#01C46C;
}


.messageList{
  background: #fff;
  width:90%;
  margin: auto;
  padding:3% 5%; 
  margin-top: 20rpx;
}
.messageListCss{
 display: flex;
 border-bottom: 1rpx solid #f5f5f5;
 justify-content: space-between;
line-height: 50rpx;
padding-bottom: 3%;
}
.message{
  display: flex;
   line-height: 50rpx;
}
.messageCss{
  margin-left: 20rpx;
}
.employee_information{
  display: flex;
  line-height: 50rpx;
  align-items: center;
}
.name{
  font-size: 30rpx;
  font-weight: 700;
}
.post{
  margin-left: 15rpx;
  font-size: 26rpx;
  color: #666;
}
.companyName{
  color: #666;
  font-size: 26rpx;
}
.main_business{
  margin-top: 10rpx;
  line-height: 46rpx;
  font-size: 28rpx;
   display: -webkit-box; 
  align-items: center;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.enter{
  background: #01C46C;
  color: white;
  font-size: 22rpx;
  padding: 5rpx 25rpx;
  border-radius: 6rpx;
}
.number{
  margin-left: 10rpx;
  color: #666;
  font-size: 24rpx;
}
.iconImg{
width: 100rpx;
height: 100rpx;
}
.pageView_Img{
  width: 30rpx;
  height: 20rpx;
}
.vip_Img{
  margin-left: 15rpx;
