page{
  background: #f5f5f5;
}
.UserInfo {
  display: flex;
  justify-content: space-around;
  background: #fff;
  padding:3% 5%;
}
.prompt{
  color: red;
}
.iconImage {
  width: 180rpx;
  height: 180rpx; 
  margin-top: 20rpx;
}
.name {
  display: flex;
  font-size: 28rpx;
  line-height: 70rpx;
  margin-left: 5%;
  align-items: center;
}
.name_input{
  width: 300rpx;
}

.get_code{
  margin-right:0rpx;
  background: #00C862;
  color: white;
  font-size: 22rpx;
  border-radius: 10rpx;
  padding: 10rpx 10rpx;
}


.company{
  margin-top: 20rpx;
  background: #fff;
  padding: 0 5%;
}
.company_name{
  display: flex;
  line-height: 80rpx;
  font-size: 28rpx;
  align-items: center;
  border-bottom:1rpx solid #f5f5f5;
}
.business_name{

  line-height: 80rpx;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  border-bottom:1rpx solid #f5f5f5;
}
.Image_jiantou{
  position: absolute;
  right: 20rpx;
  width: 16rpx;
  height: 26rpx;
  }

.input_company{
  width: 460rpx;
}
.input_business{
  width: 500rpx;
  display: -webkit-box; 
  align-items: center;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.main_product{
  margin-top: 30rpx;
  padding: 0 5% 3% 5%;
  background: #fff;
   font-size: 30rpx;
}
.main_product_title{
  line-height: 60rpx;
  font-size: 30rpx;
}
.main_product_info{
  width: 100%;
  height: 120rpx;
  border:1rpx solid #f5f5f5;
  font-size: 28rpx;
  line-height: 50rpx;
}

.main_company_profile{
  margin-top: 20rpx;
  padding: 0 5% 3% 5%;
  background: #fff;
  font-size: 30rpx;
}
.main_company_profile_info{
  width: 100%;
  height:260rpx;
  border:1rpx solid #f5f5f5;
  font-size: 28rpx;
  line-height: 50rpx;
}


.AddImgList{
width: 90%;
padding: 5% 5%;
margin: auto;
background: #fff;
margin-top: 20rpx;
}
.ThatMessageImgList{
  width: 100%;
  margin: auto;
  display: flex;
  flex-wrap: wrap;
}
.ThatMessageImg{
  width: 32%;
  margin-top: 10rpx;
  margin-left: 1%;
}
.MessageImg{
  width:100%;
  height:160rpx;
  position: relative;
}
.AddImg{
  margin-top: 5rpx;
  margin-left: 5%;
  width: 160rpx;
  height:160rpx;
}

.DelImg{
  position: absolute;
  width: 35rpx;
  height: 35rpx;
  margin-left:-30rpx;
  margin-top:-10rpx;
}

.submit{
  width: 85%;
  font-size: 32rpx;
  font-weight: bold;
  background:#01C46C;
  border-radius: 50rpx;
  color: #fff;
  margin: auto;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
}

.AddImgList_one{
width: 30%;
